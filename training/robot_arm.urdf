<?xml version="1.0" ?>
<!-- Simple 6-DOF educational robot arm for PyBullet training -->
<robot name="sb3_robot_arm">
  <material name="gray">
    <color rgba="0.7 0.7 0.7 1" />
  </material>
  <material name="blue">
    <color rgba="0.2 0.5 0.9 1" />
  </material>

  <link name="base_link">
    <visual>
      <geometry><cylinder length="0.04" radius="0.06" /></geometry>
      <material name="gray" />
    </visual>
    <collision><geometry><cylinder length="0.04" radius="0.06" /></geometry></collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001" />
    </inertial>
  </link>

  <!-- Link 1 -->
  <link name="link1">
    <visual>
      <origin xyz="0 0 0.05" />
      <geometry><box size="0.05 0.05 0.1" /></geometry>
      <material name="blue" />
    </visual>
    <collision>
      <origin xyz="0 0 0.05" />
      <geometry><box size="0.05 0.05 0.1" /></geometry>
    </collision>
    <inertial>
      <mass value="0.5" />
      <origin xyz="0 0 0.05" />
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005" />
    </inertial>
  </link>

  <!-- Link 2 -->
  <link name="link2">
    <visual>
      <origin xyz="0.1 0 0" />
      <geometry><box size="0.2 0.04 0.04" /></geometry>
      <material name="blue" />
    </visual>
    <collision>
      <origin xyz="0.1 0 0" />
      <geometry><box size="0.2 0.04 0.04" /></geometry>
    </collision>
    <inertial>
      <mass value="0.4" />
      <origin xyz="0.1 0 0" />
      <inertia ixx="0.0004" ixy="0" ixz="0" iyy="0.0004" iyz="0" izz="0.0004" />
    </inertial>
  </link>

  <!-- Link 3 -->
  <link name="link3">
    <visual>
      <origin xyz="0.1 0 0" />
      <geometry><box size="0.2 0.04 0.04" /></geometry>
      <material name="blue" />
    </visual>
    <collision>
      <origin xyz="0.1 0 0" />
      <geometry><box size="0.2 0.04 0.04" /></geometry>
    </collision>
    <inertial>
      <mass value="0.3" />
      <origin xyz="0.1 0 0" />
      <inertia ixx="0.0003" ixy="0" ixz="0" iyy="0.0003" iyz="0" izz="0.0003" />
    </inertial>
  </link>

  <!-- Link 4 -->
  <link name="link4">
    <visual>
      <origin xyz="0.08 0 0" />
      <geometry><box size="0.16 0.04 0.04" /></geometry>
      <material name="blue" />
    </visual>
    <collision>
      <origin xyz="0.08 0 0" />
      <geometry><box size="0.16 0.04 0.04" /></geometry>
    </collision>
    <inertial>
      <mass value="0.25" />
      <origin xyz="0.08 0 0" />
      <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025" />
    </inertial>
  </link>

  <!-- Link 5 -->
  <link name="link5">
    <visual>
      <origin xyz="0.06 0 0" />
      <geometry><box size="0.12 0.04 0.04" /></geometry>
      <material name="blue" />
    </visual>
    <collision>
      <origin xyz="0.06 0 0" />
      <geometry><box size="0.12 0.04 0.04" /></geometry>
    </collision>
    <inertial>
      <mass value="0.2" />
      <origin xyz="0.06 0 0" />
      <inertia ixx="0.0002" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0002" />
    </inertial>
  </link>

  <!-- Link 6 -->
  <link name="link6">
    <visual>
      <origin xyz="0.05 0 0" />
      <geometry><box size="0.1 0.04 0.04" /></geometry>
      <material name="blue" />
    </visual>
    <collision>
      <origin xyz="0.05 0 0" />
      <geometry><box size="0.1 0.04 0.04" /></geometry>
    </collision>
    <inertial>
      <mass value="0.15" />
      <origin xyz="0.05 0 0" />
      <inertia ixx="0.00015" ixy="0" ixz="0" iyy="0.00015" iyz="0" izz="0.00015" />
    </inertial>
  </link>

  <link name="end_effector">
    <visual>
      <origin xyz="0.02 0 0" />
      <geometry><sphere radius="0.02" /></geometry>
      <material name="gray" />
    </visual>
    <collision>
      <origin xyz="0.02 0 0" />
      <geometry><sphere radius="0.02" /></geometry>
    </collision>
    <inertial>
      <mass value="0.05" />
      <origin xyz="0.02 0 0" />
      <inertia ixx="0.00005" ixy="0" ixz="0" iyy="0.00005" iyz="0" izz="0.00005" />
    </inertial>
  </link>

  <!-- Revolute joints -->
  <joint name="joint1" type="revolute">
    <parent link="base_link" />
    <child link="link1" />
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit lower="-1.57" upper="1.57" effort="5" velocity="2" />
  </joint>

  <joint name="joint2" type="revolute">
    <parent link="link1" />
    <child link="link2" />
    <origin xyz="0 0 0.1" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit lower="-1.57" upper="1.57" effort="5" velocity="2" />
  </joint>

  <joint name="joint3" type="revolute">
    <parent link="link2" />
    <child link="link3" />
    <origin xyz="0.2 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit lower="-1.57" upper="1.57" effort="5" velocity="2" />
  </joint>

  <joint name="joint4" type="revolute">
    <parent link="link3" />
    <child link="link4" />
    <origin xyz="0.2 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit lower="-1.57" upper="1.57" effort="5" velocity="2" />
  </joint>

  <joint name="joint5" type="revolute">
    <parent link="link4" />
    <child link="link5" />
    <origin xyz="0.16 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit lower="-1.57" upper="1.57" effort="5" velocity="2" />
  </joint>

  <joint name="joint6" type="revolute">
    <parent link="link5" />
    <child link="link6" />
    <origin xyz="0.12 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit lower="-1.57" upper="1.57" effort="5" velocity="2" />
  </joint>

  <joint name="ee_fixed" type="fixed">
    <parent link="link6" />
    <child link="end_effector" />
    <origin xyz="0.1 0 0" rpy="0 0 0" />
  </joint>
</robot>
